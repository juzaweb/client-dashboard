{
    "name": "juzaweb/client-dashboard",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "",
    "extra": {
        "juzaweb": {
            "name": "Client Dashboard",
            "domain": "client_dashboard",
            "providers": [
                "Juzaweb\\ClientDashboard\\Providers\\ClientDashboardServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\ClientDashboard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\ClientDashboard\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1"
    },
    "require-dev": {
         "juzaweb/modules": "^1.0",
         "juzaweb/dev-tool": "^1.0"
    }
}
